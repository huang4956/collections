  "definitions": {
    "event": {
      "type": "object",
      "title": "Event",
      "description": "调用注册的Webhook url收到200 HTTP状态代码后，事件状态将更新为SUCCESS。 在成功之前，每60分钟将事件发送到注册的Webhook url，总共24次。 全部24次重试均未成功后，事件状态将更新为MAXIMUM_RETRIES_REACHED。",
      "x-tags": [
        "对象"
      ],
      "properties": {
        "id": {
          "type": "string",
          "description": "流水单号"
        },
        "topic": {
          "$ref": "#/components/schemas/eventTopicEnum"
        },
        "data": {
          "type": "object",
          "description": "Event具体对象，例如提现Event"
        },
        "createTime": {
          "type": "string",
          "description": "创建时间",
          "format": "date-time"
        },
        "status": {
          "type": "string",
          "description": "状态\n\n| status | description |\n|--------|-------------|\n| PROCESSING | 处理中 |\n| SUCCESS | 处理成功 |\n| MAXIMUM_RETRIES_REACHED | 处理失败 |",
          "enum": [
            "PROCESSING",
            "SUCCESS",
            "MAXIMUM_RETRIES_REACHED"
          ]
        },
        "webhookURL": {
          "type": "string",
          "description": "webhook url"
        }
      },
      "required": [
        "topic",
        "data",
        "createTime"
      ]
    },
    "eventTopicEnum": {
      "type": "object",
      "title": "Event-Topic枚举",
      "description": "定义的Topic\n\n| Topic | Event-Data名称| 描述|\n|--------|------------|------------|\n| withdraw.event.response | 提现Event|在提现成功、提现失败时触发相关事件|\n",
      "x-tags": [
        "对象"
      ]
    },
    "WithdrawEvent": {
      "title": "提现Event",
      "type": "object",
      "properties": {
        "externalTransId": {
          "type": "string",
          "description": "第三方单号"
        },
        "status": {
          "type": "integer",
          "description": "状态\n\nkey|name\n-|-\n2|提现成功\n3|提现失败"
        }
      },
      "required": [
        "externalTransId",
        "status"
      ],
      "x-tags": [
        "对象"
      ]
    }
