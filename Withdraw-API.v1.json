{
  "openapi": "3.0.0",
  "info": {
    "title": "3. 提现服务",
    "version": "1.0",
    "description": ""
  },
  "servers": [
    {
      "url": "https://global-api-sandbox.lianlianpay-inc.com",
      "description": "Sandbox"
    },
    {
      "url": "https://global-api.lianlianpay.com",
      "description": "Production"
    }
